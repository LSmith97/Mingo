<form id="new-post-form" method="POST" action="/posts" enctype="multipart/form-data">
    <input id="title-input" type="text" name="title" placeholder="Enter a title" required>
    <textarea id="body-input" rows="4" cols="50" maxlength="250" name="body" placeholder=" Enter your post here" required></textarea>
    <input type="file" name="imageUpload"/>
    <button type="submit">Submit Post</button>
</form>


